{
    "name": "mui-menubar",
    "type": "module",
    "version": "0.1.15",
    "description": "A React MenuBar component using Material-UI.",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "scripts": {
        "prepare": "npm run build",
        "build": "tsc",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "chromatic": "npx chromatic --project-token=chpt_63d892e309196bb",
        "release": "commit-and-tag-version --first-parent",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage"
    },
    "commit-and-tag-version": {
        "types": [
            {
                "type": "feat",
                "section": "Features"
            },
            {
                "type": "fix",
                "section": "Bug Fixes"
            },
            {
                "type": "docs",
                "section": "Documentation"
            },
            {
                "type": "chore",
                "hidden": true
            },
            {
                "type": "style",
                "hidden": true
            },
            {
                "type": "refactor",
                "hidden": true
            },
            {
                "type": "perf",
                "hidden": true
            },
            {
                "type": "test",
                "hidden": true
            }
        ],
        "ignoreCommitPattern": "^WIP|^wip|^feat: add Storybook and initial story"
    },
    "peerDependencies": {
        "@emotion/react": "^11.0.0",
        "@emotion/styled": "^11.0.0",
        "@mui/icons-material": "^5.0.0 || ^6.0.0",
