{
    "name": "mui-menubar",
    "type": "module",
    "version": "1.5.1",
    "description": "A React MenuBar component using Material-UI.",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "scripts": {
        "prepare": "npm run build",
        "build": "tsc",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "chromatic": "npx chromatic --project-token=chpt_63d892e309196bb",
        "release": "commit-and-tag-version --first-parent",
        "test": "jest --coverage",
        "test:ci": "jest --ci --coverage --maxWorkers=2",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage"
    },
    "commit-and-tag-version": {
        "types": [
            {
                "type": "feat",
                "section": "Features"
            },
            {
                "type": "fix",
                "section": "Bug Fixes"
            },
            {
                "type": "docs",
                "section": "Documentation"
            },
            {
                "type": "chore",
                "hidden": true
            },
            {
                "type": "style",
                "hidden": true
            },
            {
                "type": "refactor",
                "hidden": true
            },
            {
                "type": "perf",
                "hidden": true
            },
            {
                "type": "test",
                "hidden": true
            }
        ],
        "ignoreCommitPattern": "^WIP|^wip|^feat: add Storybook and initial story"
    },
    "peerDependencies": {
        "@emotion/react": "^11.0.0",
        "@emotion/styled": "^11.0.0",
        "@mui/icons-material": "^5.0.0 || ^6.0.0 || ^7.0.0",
        "@mui/material": "^5.0.0 || ^6.0.0 || ^7.0.0",
        "react": "^18.0.0",
        "react-dom": "^18.0.0"
    },
    "devDependencies": {
        "@babel/preset-react": "^7.26.3",
        "@babel/preset-typescript": "^7.26.0",
        "@jest/globals": "^29.7.0",
        "@storybook/addon-docs": "^9.1.2",
        "@storybook/react-vite": "^9.1.2",
        "@testing-library/jest-dom": "^6.x",
        "@testing-library/react": "^14.x",
        "@testing-library/user-event": "^14.x",
        "@types/jest": "^29.5.14",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "chromatic": "^11.12.5",
        "commit-and-tag-version": "^12.5.2",
        "jest": "^29.x",
        "jest-environment-jsdom": "^29.x",
        "storybook": "^9.1.2",
        "ts-jest": "^29.x",
        "typescript": "^5.6.3"
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE",
        "package.json"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/keithwalsh/mui-menubar.git"
    },
